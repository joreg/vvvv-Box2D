<?xml version="1.0" encoding="utf-8"?><Project    xmlns="http://schemas.microsoft.com/developer/msbuild/2003"    ToolsVersion="4.0"    DefaultTargets="Build">  <ItemGroup>    <DeployFolder Include="build\bin\" />  </ItemGroup>  <ItemGroup>    <HelpFilesLocation Include="help\*.*" />  </ItemGroup>  <Target Name="Build-x86">    <MSBuild Projects="Box2D.sln" Properties="Configuration=Release;Platform=x86;DebugSymbols=false;DebugType=None" />  </Target>  <Target Name="Build-x64">    <MSBuild Projects="Box2D.sln" Properties="Configuration=Release;Platform=x64;DebugSymbols=false;DebugType=None" />  </Target>  <Target Name="Build">    <Delete Files="build\bin\**\*.*" />        <CallTarget Targets="Build-x86" />	<Copy SourceFiles="@(HelpFilesLocation)" DestinationFolder="build\bin\Win32\" />    <Exec WorkingDirectory="build\bin\Win32\" Command="..\..\..\Tools\7zA.exe a -tzip ..\..\..\Zip\vvvv-box2d-x86.zip" LogStandardErrorAsError="true" />    <CallTarget Targets="Build-x64" />	<Copy SourceFiles="@(HelpFilesLocation)" DestinationFolder="build\bin\x64\" />	<Exec WorkingDirectory="build\bin\x64\" Command="..\..\..\Tools\7zA.exe a -tzip ..\..\..\Zip\vvvv-box2d-x64.zip" LogStandardErrorAsError="true" />  </Target></Project>